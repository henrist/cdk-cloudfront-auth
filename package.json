{
  "name": "@henrist/cdk-cloudfront-auth",
  "version": "0.0.0-development",
  "description": "CDK Constructs for adding authentication for a CloudFront Distribution",
  "repository": {
    "type": "git",
    "url": "https://github.com/henrist/cdk-cloudfront-auth"
  },
  "scripts": {
    "build": "rimraf dist && webpack && tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prepare": "npm run build",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "cdk",
    "cloudfront",
    "authentication"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist/**/*",
    "lib/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.91.0",
    "@aws-cdk/aws-cloudfront": "1.91.0",
    "@aws-cdk/aws-cognito": "1.91.0",
    "@aws-cdk/aws-iam": "1.91.0",
    "@aws-cdk/aws-lambda": "1.91.0",
    "@aws-cdk/aws-s3": "1.91.0",
    "@aws-cdk/core": "1.91.0",
    "@aws-cdk/custom-resources": "1.91.0",
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@types/aws-lambda": "8.10.75",
    "@types/cookie": "0.4.0",
    "@types/jest": "26.0.22",
    "@types/jsonwebtoken": "8.5.1",
    "@types/node": "14.14.37",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "aws-sdk": "2.799.0",
    "axios": "0.21.1",
    "cookie": "0.4.1",
    "eslint": "7.23.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-deprecation": "1.2.0",
    "eslint-plugin-prettier": "3.3.1",
    "html-loader": "1.3.2",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-cdk-snapshot": "1.4.1",
    "jsonwebtoken": "8.5.1",
    "jwks-rsa": "1.12.3",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.2",
    "ts-jest": "26.5.4",
    "ts-loader": "8.1.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.4",
    "webpack": "5.30.0",
    "webpack-cli": "4.6.0"
  },
  "dependencies": {
    "@henrist/cdk-cross-region-params": "^1.0.0",
    "@henrist/cdk-lambda-config": "^1.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "peerDependencies": {
    "@aws-cdk/aws-cloudfront": "^1.72.0",
    "@aws-cdk/aws-cognito": "^1.72.0",
    "@aws-cdk/aws-iam": "^1.72.0",
    "@aws-cdk/aws-lambda": "^1.72.0",
    "@aws-cdk/aws-ssm": "^1.72.0",
    "@aws-cdk/core": "^1.72.0",
    "@aws-cdk/custom-resources": "^1.72.0"
  }
